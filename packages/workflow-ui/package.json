{
  "name": "@gocodealoneorg/workflow-ui",
  "version": "0.1.0",
  "description": "Shared UI component library for GoCodeAlone workflow applications",
  "type": "module",
  "main": "./dist/workflow-ui.umd.cjs",
  "module": "./dist/workflow-ui.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/workflow-ui.js",
      "require": "./dist/workflow-ui.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@xyflow/react": ">=12.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "zustand": ">=5.0.0"
  },
  "dependencies": {
    "dagre": "^0.8.5",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/dagre": "^0.7.53",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@xyflow/react": "^12.10.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "zustand": "^5.0.11"
  },
  "keywords": [
    "workflow",
    "ui",
    "components",
    "react",
    "gocodealoneorg"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GoCodeAlone/workflow"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
