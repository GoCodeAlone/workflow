{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Overview of the Workflow Engine: execution rates, latency, error rates, and resource usage.",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Workflow Executions / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(workflow_executions_total[5m])) by (workflow_type, status) * 60",
          "legendFormat": "{{workflow_type}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "opm"
        }
      }
    },
    {
      "title": "Workflow Duration p99",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(workflow_duration_seconds_bucket[5m])) by (le, workflow_type))",
          "legendFormat": "{{workflow_type}} p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Error Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(workflow_executions_total{status=\"error\"}[5m])) / sum(rate(workflow_executions_total[5m]))",
          "legendFormat": "Error Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.01 },
              { "color": "red", "value": 0.05 }
            ]
          }
        }
      }
    },
    {
      "title": "Active Workflows",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 8 },
      "targets": [
        {
          "expr": "sum(active_workflows)",
          "legendFormat": "Active"
        }
      ]
    },
    {
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m])) by (method, status_code) * 60",
          "legendFormat": "{{method}} {{status_code}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "opm"
        }
      }
    },
    {
      "title": "HTTP Request Latency p95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "{{path}} p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Module Operations",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "sum(rate(module_operations_total[5m])) by (module, status) * 60",
          "legendFormat": "{{module}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "opm"
        }
      }
    },
    {
      "title": "Process Memory Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=\"workflow\"}",
          "legendFormat": "RSS"
        },
        {
          "expr": "go_memstats_alloc_bytes{job=\"workflow\"}",
          "legendFormat": "Go Heap Alloc"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "title": "Go Goroutines",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "go_goroutines{job=\"workflow\"}",
          "legendFormat": "Goroutines"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["workflow", "overview"],
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus"
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "title": "Workflow Engine Overview",
  "uid": "workflow-overview"
}
