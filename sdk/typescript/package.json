{
  "name": "@gocodalone/workflow-sdk",
  "version": "0.1.0",
  "description": "TypeScript client SDK for the GoCodeAlone Workflow engine",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
