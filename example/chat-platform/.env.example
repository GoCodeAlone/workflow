# Chat Platform Environment Variables
# Copy this file to .env and fill in the values before running docker compose.

# JWT signing secret (minimum 32 characters, use a cryptographically random value)
# Generate with: openssl rand -hex 32
JWT_SECRET=

# AES-256 encryption key for PII data (exactly 32 characters)
# Generate with: openssl rand -hex 16
ENCRYPTION_KEY=

# Grafana admin password (optional, defaults to 'admin')
GRAFANA_ADMIN_PASSWORD=
